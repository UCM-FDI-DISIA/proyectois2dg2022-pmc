                <ATTRIBUTES>
                  <ATT name="Content"><![CDATA[// Formato para el guardado, todos cubos se guardan como "color posx posy \n" y
// al final tenemos "Player  color" para el jugador que tenia el turno.]]></ATT>
                  <ATT name="MimeType"><![CDATA[text/plain]]></ATT>
                  <ATT name="Name"></ATT>
                  <ATT name="status">1970354901745664</ATT>
                </ATTRIBUTES>
                <DEPENDENCIES>
                  <LINK relation="Model">
                    <FOREIGNID name="JavaComment" mc="Infrastructure.NoteType" uid="69ab88b5-741c-11e1-89e3-002564c97630"/>
                  </LINK>
